{
  "all": true,
  "report-dir": "./reports/coverage-default",
  "check-coverage": false,
  "exclude": [
    "lib/**/*.test.js",
    "reports",
    "*.js"
  ],
  "reporter": [
    "cobertura",
    "html",
    "text",
    "text-summary"
  ],
  "watermarks": {
    "statements": [50, 80],
    "lines": [50, 80],
    "functions": [50, 80],
    "branches": [50, 80]
  }
}
